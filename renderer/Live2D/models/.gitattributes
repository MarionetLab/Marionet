# Live2D模型文件使用Git LFS
*.moc3 filter=lfs diff=lfs merge=lfs -text
*.model3.json filter=lfs diff=lfs merge=lfs -text
*.motion3.json filter=lfs diff=lfs merge=lfs merge=lfs -text
*.exp3.json filter=lfs diff=lfs merge=lfs -text
*.physics3.json filter=lfs diff=lfs merge=lfs -text
*.pose3.json filter=lfs diff=lfs merge=lfs -text
*.cdi3.json filter=lfs diff=lfs merge=lfs -text
*.can3 filter=lfs diff=lfs merge=lfs -text
*.cmo3 filter=lfs diff=lfs merge=lfs -text

# 纹理文件使用Git LFS
*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text

# 其他大文件
*.dll filter=lfs diff=lfs merge=lfs -text
*.lib filter=lfs diff=lfs merge=lfs -text
*.exp filter=lfs diff=lfs merge=lfs -text
